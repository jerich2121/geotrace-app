{
  "version": 2,
  "functions": {
    "api/**/*.js": {
      "runtime": "nodejs20.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/auth/login",
      "destination": "/api/auth/login.js"
    },
    {
      "source": "/api/auth/me",
      "destination": "/api/auth/me.js"
    },
    {
      "source": "/api/history",
      "destination": "/api/history/index.js"
    }
  ]
}